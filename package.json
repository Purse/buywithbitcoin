    "mem": "^5.1.1",
    "react": "^16.3.0",
    "react-chrome-redux": "^2.0.0-alpha.5",
    "react-dom": "^16.9.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
